

{
	AWSTemplateFormatVersion": "2010-09-09",
	"Description": "1 CD, 1 Security Group, and 1 subnet.",
	"Parameters": {
	    "InstanceCount": {
		"Type": "Number",
		"Default": 1,
		"Description": "Number of desired instances in Auto Scaling Group"
	    }
	},
	"Resources": {
        "appLoadBalancer" : {
            "Type" : "AWS::ElasticLoadBalancingV2::LoadBalancer",
            "Properties" : {    
                    "Name" : "BlueGreen",
                    "Scheme" : "internet-facing",
                    "SecurityGroups" : ["sg-36b76a4e"],
                    "Subnets" : [ "subnet-597ca073", "subnet-dfac0d87"],
                    "Type" : "application",
                    "IpAddressType" : "ipv4"
            }   
        }       
    }
}
